<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.me.riemann.health</string>
        <key>Disabled</key>
        <false/>
        <key>RunAtLoad</key>
        <true/>
        <key>KeepAlive</key>
        <true/>
        <key>ProgramArguments</key>
        <array>
            <string>/Users/achambers/.rbenv/shims/riemann-health</string>
        </array>
        <key>StartInterval</key>
       	<integer>30</integer>
				<key>StandardErrorPath</key>
				<string>/Users/achambers/logs/riemann-health.log</string>
				<key>StandardOutPath</key>
				<string>/Users/achambers/logs/riemann-health.log</string>
    </dict>
</plist>
